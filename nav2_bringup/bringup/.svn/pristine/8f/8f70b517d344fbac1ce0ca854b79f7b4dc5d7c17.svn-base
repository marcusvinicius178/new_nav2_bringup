      <inertia ixx="6.229030166666667" ixy="0.0" ixz="0.0" iyy="6.229030166666667" iyz="0.0" izz="10.629952000000001"/>
    </inertial>
  </link>
  <!-- First and fourth wheels are continuous and children of the steering joints/base link -->
  <joint name="fourth_wheel_left_joint" type="continuous">
    <parent link="fourth_shaft_horizontal_shaft_link"/>
    <origin rpy="0 0 1.57" xyz="1.299 0 0"/>
    <child link="fourth_wheel_left_link"/>
    <axis xyz="0 -1 0"/>
  </joint>
  <link name="fourth_wheel_right_link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.385" radius="0.536"/>
      </geometry>
      <material name="Gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <!-- Add mass and inertia -->
    <!-- Add mass and inertia -->
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="74"/>
      <inertia ixx="0.5 * 74 * 0.536 * 0.536" ixy="0" ixz="0" iyy="0.5 * 74 * 0.536 * 0.536" iyz="0" izz="74 * 0.536 * 0.536"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder length="0.385" radius="0.536"/>
      </geometry>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <surface>
        <friction>
          <ode>
            <mu>1.0</mu>
            <mu2>1.0</mu2>
            <kp>5000000.0</kp>
            <kd>1.0</kd>
            <fdir1>1 0 0</fdir1>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="74"/>
      <inertia ixx="6.229030166666667" ixy="0.0" ixz="0.0" iyy="6.229030166666667" iyz="0.0" izz="10.629952000000001"/>
    </inertial>
  </link>
  <!-- First and fourth wheels are continuous and children of the steering joints/base link -->
  <joint name="fourth_wheel_right_joint" type="continuous">
    <parent link="fourth_shaft_horizontal_shaft_link"/>
    <origin rpy="0 0 1.57" xyz="-1.299 0 0"/>
    <child link="fourth_wheel_right_link"/>
    <axis xyz="0 -1 0"/>
  </joint>
  <!--Sensors -->
  <!-- Imu Link-->
  <link name="imu_link">
    <visual>
      <geometry>
        <box size="0.25 0.25 0.25"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.25 0.25"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 1.5707963267948966" xyz="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0001666666666666667" ixy="0.0" ixz="0.0" iyy="0.0001666666666666667" iyz="0.0" izz="0.0001666666666666667"/>
    </inertial>
  </link>
  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0.4 0 0.5"/>
  </joint>
  <!-- LiDAR support base link -->
  <!-- <link name="lidar_base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.6 0.6 0.3"/>
      </geometry>
